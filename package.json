{
  "name": "popodou",
  "version": "0.0.0",
  "description": "Simple project that shows the usage of Next.js with TypeScript",
  "author": "TOKTOKHAN.DEV<dev@toktokhan.dev>",
  "scripts": {
    "dev": "pnpm theme && next dev",
    "build": "pnpm theme && next build",
    "analyze": "ANALYZE=true next build",
    "type-check": "tsc --skipLibCheck --noEmit",
    "lint": "pnpm type-check && next lint",
    "lint:staged": "lint-staged",
    "start": "pnpm build && next start",
    "test": "jest --coverage --coverageReporters=text-summary",
    "test:watch": "jest --watch",
    "clean-up": "node src/scripts/remove-unnecessary.js",
    "tokript": "tokript2",
    "commit": "tokript2 commit",
    "theme": "npx chakra typegen src/configs/theme/index.ts",
    "theme:open": "code -r node_modules/@chakra-ui/styled-system/dist/theming.types.d.ts",
    "api:gen": "npx orval --config orval.config.ts",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "semantic-release": "semantic-release",
    "checklist:progress": "node src/scripts/update-checklist-progress.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "3.13.0",
    "@emotion/react": "11.14.0",
    "@hookform/resolvers": "^3.10.0",
    "@phosphor-icons/react": "^2.1.10",
    "@tanstack/react-query": "5.74.7",
    "@tanstack/react-query-devtools": "5.74.7",
    "@toktokhan-dev/react-universal": "^0.0.12",
    "@toktokhan-dev/react-web": "^0.0.23",
    "@toktokhan-dev/universal": "^0.0.8",
    "@toktokhan-dev/zustand-react": "^0.0.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react-responsive-masonry": "^2.6.0",
    "axios": "^1.11.0",
    "chakra-react-select": "6.0.0",
    "dayjs": "^1.11.13",
    "immer": "^10.1.1",
    "isomorphic-dompurify": "^2.26.0",
    "jose": "^6.0.12",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "next": "^14.2.32",
    "next-themes": "^0.4.6",
    "overlay-kit": "^1.8.4",
    "react": "^18.3.1",
    "react-cookie": "^7.2.2",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-input-autosize": "^3.0.0",
    "react-intersection-observer": "^9.16.0",
    "react-responsive-masonry": "^2.7.1",
    "react-use": "^17.6.0",
    "yup": "^1.7.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@chakra-ui/cli": "3.2.3",
    "@next/bundle-analyzer": "^14.2.32",
    "@next/eslint-plugin-next": "^14.2.32",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@toktokhan-dev/cli": "^0.0.11",
    "@toktokhan-dev/cli-plugin-commit": "0.0.10",
    "@toktokhan-dev/cli-plugin-gen-api-react-query": "0.1.6",
    "@toktokhan-dev/cli-plugin-gen-icon-chakra": "0.0.13",
    "@toktokhan-dev/cli-plugin-gen-img": "^0.0.11",
    "@toktokhan-dev/cli-plugin-gen-theme-chakra": "^0.0.17",
    "@toktokhan-dev/eslint-config": "^0.0.2",
    "@toktokhan-dev/prettier-config": "^0.0.1",
    "@toktokhan-dev/ts-config": "^0.0.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.14.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-input-autosize": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 7.10.0",
    "pnpm": ">= 8.15.9"
  },
  "os": [
    "darwin",
    "win64",
    "linux"
  ],
  "engineStrict": true,
  "packageManager": "pnpm@10.4.0",
  "pnpm": {
    "patchedDependencies": {
      "lodash@4.17.21": "patches/lodash@4.17.21.patch"
    }
  }
